#print a number 5 times
for i in range(1,6):
    for j in range(1,11):
        print(j,end="")
    print()
#checking the number of charas repeated
string=input("Enter a string : ")
char = input("Enter a character to find its frequency : ")
i = 0
count = 0
while i < len(string):
    if string [i] == char:
        count = count + 1
    i = i + 1
print("The frequency of the character is :", count)
#checking if number is prime
lowerrange = int(input("Enter lower range : "))
upperrange = int(input("Enter upper range : "))
for num in range (lowerrange, upperrange + 1):
    if num > 1:
        for i in range (2, num):
            if (num % i) == 0:
                break
        else:
            print(num, "is a prime number")
#midproduct
num = int(input("Enter a number : "))
t = num
numlength = 0
while t > 0:
    numlength = numlength + 1
    t = int(t / 10)
if numlength >= 4:
    numlength = int(numlength / 2)
    check = 0
    while num > 0:
        rem = num % 10
        if check == numlength:
            mid1 = rem
        elif check == numlength - 1:
            mid2 = rem
        num = int(num / 10)
        check = check + 1
    midproduct = mid1 + mid2
    print("The product is :", midproduct)
else:
    print("Number length is less than 4")
