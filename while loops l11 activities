#sum of natural numbers
n=int(input("Enter a number: "))
sum=0
i=1
while i<=n:
    sum=sum+i
    i=i+1
print("The sum of first",n,"natural numbers is:",sum)
#infinity
i=7
while i <= 0:
    print("This loop will run forever")
#armstrong number
n= int(input("Enter a number: "))
sum = 0
temp = n
while temp > 0:
    digit = temp % 10
    sum += digit ** 3
    temp //= 10
if n == sum:
    print(n,"is an Armstrong number")
else:
    print(n,"is not an Armstrong number")
